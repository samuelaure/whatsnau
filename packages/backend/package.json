{
  "name": "@whatsnau/backend",
  "version": "4.14.0",
  "description": "Campaign-driven WhatsApp CRM & Automation platform",
  "type": "module",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src/**",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "tsx prisma/seed.ts",
    "test": "SET NODE_ENV=test&& vitest run",
    "test:watch": "SET NODE_ENV=test&& vitest",
    "test:coverage": "SET NODE_ENV=test&& vitest run --coverage"
  },
  "keywords": [
    "whatsapp",
    "crm",
    "automation",
    "openai",
    "campaign"
  ],
  "author": {
    "name": "Samuel Aure",
    "url": "https://www.samuelaure.com"
  },
  "license": "Proprietary",
  "engines": {
    "node": ">=20.11.0"
  },
  "dependencies": {
    "@prisma/client": "^5.8.1",
    "@types/pino": "^7.0.4",
    "bcryptjs": "^3.0.3",
    "body-parser": "^2.2.2",
    "bullmq": "^5.67.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "dotenv": "^16.3.1",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.9.2",
    "jsonwebtoken": "^9.0.3",
    "openai": "^6.16.0",
    "pino": "^10.2.1",
    "pino-pretty": "^13.1.3",
    "prisma": "^5.8.1",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/body-parser": "^1.19.6",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/ioredis": "^4.28.10",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.11.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "@vitest/coverage-v8": "^4.0.17",
    "eslint": "^9.39.2",
    "prettier": "^3.8.0",
    "supertest": "^7.2.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^4.0.17"
  }
}
